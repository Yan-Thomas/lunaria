  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "prettier-plugin-organize-imports": "^3.2.3"
  },
  "packageManager": "pnpm@8.7.6"
}
